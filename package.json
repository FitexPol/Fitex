{
  "name": "Fitex",
  "version": "1.0.50",
  "module": "src/index.ts",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" & exit 1",
    "dev": "concurrently \"bun run --watch src/index.ts\" \"bun run tw\"",
    "tw": "tailwindcss -i ./src/styles.css -o ./public/styles.css --watch",
    "tslint": "tsc --noEmit",
    "lint": "eslint . & bun run tslint",
    "lint:fix": "eslint --fix . & bun run tslint",
    "format": "prettier --check .",
    "format:fix": "prettier --write ."
  },
  "dependencies": {
    "@elysiajs/html": "^0.7.0",
    "@elysiajs/static": "^0.7.1",
    "@elysiajs/swagger": "^0.7.3",
    "elysia": "^0.7.0"
  },
  "devDependencies": {
    "@kitajs/ts-html-plugin": "^1.3.0",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "bun-types": "^1.0.6",
    "concurrently": "^8.2.1",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.5",
    "tailwindcss": "^3.3.3"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
